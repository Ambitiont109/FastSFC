{
  "name": "FastSFC",
  "version": "0.1.0",
  "description": "FastSFC client",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "client": "cross-env NODE_ENV=development webpack-dev-server --open",
    "deploy": "npm run-script deploy:web; npm run-script deploy:compute",
    "deploy:web": "cd ansible && ansible-playbook -i production web.yml --tags=deploy",
    "deploy:compute": "cd ansible && ansible-playbook -i production compute.yml --tags=deploy",
    "flake8": "flake8 app",
    "format": "prettier --write 'packages/**/*.js'",
    "precommit": "lint-staged && npm run-script build && git add --all",
    "server": "python3 manage.py runserver 0.0.0.0:8000",
    "ssh:web": "bash ssh web",
    "ssh:compute": "bash ssh compute",
    "start": "run-p watch server",
    "test": "jest --watchAll --coverage",
    "watch": "cross-env NODE_ENV=development webpack --watch"
  },
  "lint-staged": {
    "client/*.{js,jsx,scss,css,md}": [
      "prettier --write --single-quote",
      "eslint --fix"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(jpg|gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss)$": "<rootDir>/node_modules/jest-css-modules-transform"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js",
    "moduleFileExtensions": [
      "css",
      "scss",
      "js",
      "json",
      "jsx"
    ]
  },
  "repository": "https://justinyek@bitbucket.org/justinyek/fast.git",
  "author": "FastSFC",
  "license": "ISC",
  "private": false,
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "dependencies": {
    "bootstrap-sass": "3.4.1",
    "font-awesome": "4.7.0",
    "html-to-react": "^1.3.4",
    "jquery": "3.3.1",
    "lodash": "4.17.19",
    "moment": "^2.24.0",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.14.7",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-hot-loader": "4.6.3",
    "throttle-debounce": "^2.1.0"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.2.3",
    "@babel/plugin-proposal-decorators": "7.2.3",
    "@babel/plugin-proposal-export-namespace-from": "7.2.0",
    "@babel/plugin-proposal-function-sent": "7.2.0",
    "@babel/plugin-proposal-json-strings": "7.2.0",
    "@babel/plugin-proposal-numeric-separator": "7.2.0",
    "@babel/plugin-proposal-throw-expressions": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-import-meta": "7.2.0",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/polyfill": "7.2.5",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.0.0",
    "@babel/runtime": "7.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "babel-plugin-lodash": "3.3.4",
    "browserslist": "4.3.7",
    "clean-webpack-plugin": "1.0.0",
    "connect-history-api-fallback": "^1.5.0",
    "cross-env": "5.2.0",
    "css-loader": "2.1.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "eslint": "5.12.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "22.1.2",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.3",
    "eslint-watch": "4.0.2",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "1.3.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-css-modules-transform": "^2.0.2",
    "jest-enzyme": "7.0.1",
    "jest-fetch-mock": "^2.0.1",
    "jsdom": "^13.0.0",
    "koa-connect": "^2.0.1",
    "lint-staged": "8.1.0",
    "mini-css-extract-plugin": "0.5.0",
    "node-sass": "4.11.0",
    "npm-check-updates": "^2.14.2",
    "prettier": "1.15.3",
    "pretty-quick": "1.8.0",
    "prop-types": "^15.6.2",
    "resolve-url-loader": "^3.0.1",
    "sass-loader": "7.1.0",
    "script-ext-html-webpack-plugin": "^2.1.3",
    "style-loader": "0.23.1",
    "webpack": "4.28.1",
    "webpack-bundle-tracker": "^0.4.2-beta",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "3.1.14",
    "webpack-merge": "4.2.1"
  }
}
